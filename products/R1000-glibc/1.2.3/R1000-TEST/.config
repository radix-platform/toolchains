#!/bin/bash

#################################################################
#
# default common variables:
#
if test -z "$HOST"; then
  export HOST=x86_64-pc-linux-gnu
fi

if test -z "$TARGET"; then
  export TARGET=sparc64-r1000-linux-gnu
fi

if test -z "$TOOLCHAIN_PATH"; then
  export TOOLCHAIN_PATH=/opt/toolchains/sparc64-R1000-linux-glibc/1.2.3
fi

#
# END of default common variables.
#
#################################################################
