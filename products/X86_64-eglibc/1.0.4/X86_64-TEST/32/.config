#!/bin/sh

#################################################################
#
# default common variables:
#
if test -z "$HOST"; then
  #export HOST=i586-pc-cygwin
  export HOST=x86_64-slamd64-linux
fi

if test -z "$TARGET"; then
  export TARGET=x86_64-kxLab-linux-gnu
fi

