#!/bin/sh

#################################################################
#
# default common variables:
#
if test -z "$HOST"; then
  #export HOST=i586-pc-cygwin
  export HOST=x86_64-pc-linux-gnu
fi

if test -z "$TARGET"; then
  export TARGET=arm-dm644x-linux-gnueabi
fi

if test -z "$TOOLCHAIN_PATH"; then
  export TOOLCHAIN_PATH=/opt/toolchain/arm-DM644X-linux-glibc/1.0.9
fi

#
# END of default common variables.
#
#################################################################
