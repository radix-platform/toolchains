#!/bin/sh

#################################################################
#
# default common variables:
#
if test -z "$HOST"; then
  export HOST=x86_64-slamd64-linux
fi

if test -z "$TARGET"; then
  export TARGET=i486-radix-linux-gnu
fi

if test -z "$TOOLCHAIN_PATH"; then
  export TOOLCHAIN_PATH=/opt/toolchain/i486-PC-linux-glibc/1.0.10
fi

#
# END of default common variables.
#
#################################################################
